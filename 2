</script> 
<script>    
$(document).ready(function () 
{         
$("#but1").click(function () 
{             
$(".MsoNormal").hide();         
});         
$("#but2").click(function () 
{             
$(".MsoNormal").show();         
      });     
     });
</script> 
</head> 
